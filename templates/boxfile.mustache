build:
  lib_dirs:
    - env
{{#can_run}}
web1:
  ports:
    - http:80:8080
    - http:443:8080
  exec: {{code_dir}}/env/bin/gunicorn -c {{etc_dir}}/gunicorn/config.py {{app_module}}
  log_watch:
    gunicorn[access]: {{data_dir}}/var/log/gunicorn/access.log
    gunicorn[error]: {{data_dir}}/var/log/gunicorn/error.log
{{/can_run}}